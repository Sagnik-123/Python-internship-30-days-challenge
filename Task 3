# Write a Python Script To Merge Two Dictionaries

a = {'India': 'Asia', 'South Africa': 'Africa'}
b = {'Belgium': 'Europe', 'New Zealand': 'Australia'}
ans = a.copy()
ans.update(b)
print(ans)

# Write a Python program to remove a key from a dictionary

Capitals = {"Maharashtra": "Mumbai", "Telangana": "Hyderabad", "Tamilnadu": "Chennai"}
del Capitals["Maharashtra"]
print(Capitals)

# Write a Python Program to map two lists into a dictionary

keys = ['Ronaldo', 'Messi', 'Rooney']
values = ['11', '10', '21']
dictionary = dict(zip(keys, values))
print(dictionary)

# Write a Python program to find length of a set

segment = set([5, 20, 14, 18, 4, 9])
print(len(segment))

# or

CSK = {"Dhoni", "Bravo", "Jadeja"}
print(len(CSK))

# Write a Python program to remove the intersection of a 2nd set from the 1st set

s1 = {1, 2, 3, 4, 5}
s2 = {4, 5, 6, 7, 8}
print("Remove the intersection of a 2nd set from 1st set")
print(s1-s2)
